cmake_minimum_required(VERSION 3.28.3)

project(
    file_creat
    LANGUAGES C
    VERSION 1.0
    DESCRIPTION "file creat test"
)

#搜索源文件
aux_source_directory(. SRC)

add_executable(demo ${SRC})
set(EXECUTABLE_OUTPUT_PATH ..)
